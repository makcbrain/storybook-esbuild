{
	"name": "@makcbrain/storybook-builder-esbuild",
	"version": "1.0.2",
	"author": "Maksim Kadochnikov <makÑ.brain@gmail.com>",
	"description": "Builder for Storybook with supporting esbuild",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"storybook",
		"esbuild",
		"builder",
		"react"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/makcbrain/storybook-esbuild.git",
		"directory": "packages/builder-esbuild"
	},
	"bugs": {
		"url": "https://github.com/makcbrain/storybook-esbuild/issues"
	},
	"homepage": "https://github.com/makcbrain/storybook-esbuild/tree/main/packages/builder-esbuild",
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"assets",
		"LICENSE",
		"package.json"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "rm -rf dist && bunx tsc",
		"prepublishOnly": "(cd ../../ && bun run precommit) && bun run build"
	},
	"dependencies": {
		"@fal-works/esbuild-plugin-global-externals": "2.1.2",
		"@mdx-js/esbuild": "3.1.1",
		"dedent": "1.7.0",
		"glob": "11.0.3",
		"react-docgen": "8.0.2",
		"slash": "5.1.0"
	},
	"peerDependencies": {
		"esbuild": "^0.10.0"
	}
}
