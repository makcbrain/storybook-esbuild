{
	"name": "@makcbrain/storybook-framework-react-esbuild",
	"version": "0.1.0",
	"author": "Maksim Kadochnikov <makс.brain@gmail.com>",
	"description": "Framework for Storybook with supporting React and esbuild",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"LICENSE",
		"package.json"
	],
	"peerDependencies": {
		"@storybook/react": "^9.1.7"
	},
	"dependencies": {
		"@makcbrain/storybook-builder-esbuild": "workspace:*"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./preset": {
			"types": "./dist/preset.d.ts",
			"default": "./dist/preset.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "rm -rf dist && bunx tsc"
	}
}
